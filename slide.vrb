			run_MC <- function(r, nsize) {
				set.seed(r)
				n <- nsize
				betas <- c(1.2, 0.25, -0.08, 0.15, -0.12)
				
				df <- data.frame(
				x1 = sample(c(0, 1), n, replace = T, prob = c(0.7, 0.3)),
				x2 = rnorm(n),
				x3 = rnorm(n),
				x4 = sample(c(0, 1), n, replace = T, prob = c(0.2, 0.8))
				)
				X <- model.matrix(~., df)
				eta <- X %*% betas
				mu <- exp(eta)
			\end{lstlisting}
