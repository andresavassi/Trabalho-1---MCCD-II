\frametitle{Amostrador Bell via Box Muller}
\begin{columns}
			
			\begin{column}{0.85\textwidth}
				\begin{lstlisting}[language=R]
					n <- 10000
					
					u1 <- runif(n)
					u2 <- runif(n)
					
					z1 <- sqrt(-2 * log(u1)) * cos(2 * pi * u2)
					z2 <- sqrt(-2 * log(u1)) * sin(2 * pi * u2)
					
					x_bm <- c(z1, z2)
				\end{lstlisting}
